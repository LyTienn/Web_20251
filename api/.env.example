PORT=5000
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# JWT Secrets - Generate your own using: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your-secret-key-here
JWT_REFRESH_SECRET=your-refresh-secret-key-here-must-be-different

# Token Expiration
ACCESS_TOKEN_EXPIRE=15m
REFRESH_TOKEN_EXPIRE=7d

# Environment
NODE_ENV=development